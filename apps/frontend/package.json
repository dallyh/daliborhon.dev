{
    "name": "@daliborhon.dev/frontend",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "astro": "astro",
        "sync": "astro sync",
        "dev": "astro dev",
        "dev:host": "astro dev --host",
        "dev:tunnel": "pnpm lt --port 4321 --subdomain daliborhondev",
        "build": "astro build",
        "build:devdata": "set BUILD_DEV_DATASET=true && astro build",
        "build:turbo": "dotenv -- turbo build",
        "build:index": "pagefind --site dist",
        "build-and-preview": "astro build && pnpm run preview",
        "preview": "wrangler pages dev ./dist --compatibility-flags='nodejs_compat' --live-reload --port 4321",
        "compile:paraglide": "paraglide-js compile --project ./project.inlang",
        "wrangler:types": "wrangler types ./src/worker-configuration.d.ts"
    },
