{
    "name": "@daliborhon.dev/frontend",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "init": "pnpm run wrangler:types && pnpm run build",
        "dev": "astro dev",
        "dev:host": "astro dev --host",
        "build": "astro build",
        "build-and-preview": "astro build && pnpm run preview",
        "preview": "wrangler pages dev ./dist --compatibility-flags='nodejs_compat' --live-reload --port 4321",
        "astro": "astro",
        "sync": "astro sync",
        "build:index": "pagefind --site dist",
        "compile:paraglide": "paraglide-js compile --project ./project.inlang",
        "wrangler:types": "wrangler types ./src/worker-configuration.d.ts",
        "deps:check-update": "pnpm ncu -x @astrojs/cloudflare",
        "deps:update": "pnpm ncu -u -x @astrojs/cloudflare && pnpm install"
    },
    "dependencies": {
        "@astro-community/astro-embed-youtube": "^0.5.2",
        "@astrojs/cloudflare": "9.2.0",
        "@astrojs/react": "^3.3.0",
        "@astrojs/rss": "^4.0.5",
        "@astrojs/tailwind": "^5.1.0",
        "@formkit/auto-animate": "^0.8.2",
        "@hcaptcha/react-hcaptcha": "^1.10.1",
        "@iconify-json/bi": "^1.1.23",
        "@iconify-json/devicon": "^1.1.37",
        "@iconify-json/tabler": "^1.1.110",
        "@inlang/paraglide-js-adapter-astro": "^0.1.28",
        "@pagefind/default-ui": "^1.1.0",
        "@portabletext/to-html": "^2.0.13",
        "@resvg/resvg-js": "^2.6.2",
        "@sanity/astro": "^3.0.0",
        "@sanity/client": "^6.15.13",
        "@sanity/image-url": "^1.0.2",
        "@tailwindcss/nesting": "0.0.0-insiders.565cd3e",
        "@tailwindcss/typography": "^0.5.12",
        "@unpic/astro": "^0.0.44",
        "astro": "^4.6.1",
        "astro-expressive-code": "^0.34.1",
        "astro-icon": "^1.1.0",
        "astro-pagefind": "^1.5.0",
        "astro-portabletext": "^0.9.7",
        "focus-trap": "^7.5.4",
        "groq": "^3.37.2",
        "groqd": "^0.15.10",
        "htm": "^3.1.1",
        "iso-639-1": "^3.1.2",
        "js-cookie": "^3.0.5",
        "jsdom": "^24.0.0",
        "pagefind": "^1.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.51.3",
        "shikiji": "^0.10.2",
        "tailwindcss": "^3.4.3",
        "tippy.js": "^6.3.7",
        "vhtml": "^2.2.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@astrojs/sitemap": "^3.1.2",
        "@cloudflare/workers-types": "^4.20240405.0",
        "@daliborhon.dev/shared": "workspace:*",
        "@daliborhon.dev/studio": "workspace:*",
        "@inlang/paraglide-js": "1.3.5",
        "@types/js-cookie": "^3.0.6",
        "@types/jsdom": "^21.1.6",
        "@types/react": "^18.2.78",
        "@types/react-dom": "^18.2.25",
        "@types/slug": "^5.0.8",
        "@types/vhtml": "^2.2.9",
        "autoprefixer": "^10.4.19",
        "md-to-pdf": "^5.2.4",
        "postcss-import": "^16.1.0",
