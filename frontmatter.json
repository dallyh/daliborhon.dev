{
    "$schema": "https://frontmatter.codes/frontmatter.schema.json",
    "frontMatter.framework.id": "other",
    "frontMatter.preview.host": "http://localhost:4321",
    "frontMatter.preview.trailingSlash": false,
    "frontMatter.content.publicFolder": {
        "relative": true,
        "path": "[[workspace]]/www/content"
    },
    "frontMatter.content.supportedFileTypes": ["md", "mdx", "markdown", "markdoc", "mdoc"],
    "frontMatter.media.contentTypes": [
        {
            "name": "default",
            "fields": [
                {
                    "title": "Title",
                    "name": "title",
                    "type": "string"
                },
                {
                    "title": "Author",
                    "name": "author",
                    "type": "string"
                },
                {
                    "title": "Alt text",
                    "name": "alt",
                    "type": "string"
                }
            ]
        }
    ],
    "frontMatter.data.files": [
        {
            "id": "projectTags",
            "labelField": "id",
            "title": "Project tags",
            "file": "[[workspace]]/www/content/project-tags.yaml",
            "fileType": "yaml",
            "schema": {
                "type": "object",
                "title": "Tag",
                "properties": {
                    "id": {
                        "type": "string",
                        "title": "Id"
                    },
                    "title": {
                        "type": "string",
                        "title": "Title"
                    },
                    "bgColor": {
                        "type": "string",
                        "title": "Background color"
                    }
                }
            }
        },
        {
            "id": "tags",
            "labelField": "id",
            "title": "Blog tags",
            "file": "[[workspace]]/www/content/tags.yaml",
            "fileType": "yaml",
            "schema": {
                "type": "object",
                "title": "Tag",
                "properties": {
                    "id": {
                        "type": "string",
                        "title": "Id"
                    }
                }
            }
        }
    ],
    "frontMatter.content.pageFolders": [
        {
            "title": "Blog posts",
            "path": "[[workspace]]/www/content/posts",
            "contentTypes": ["blog"],
            "filePrefix": "{{year}}-{{month}}-{{day}}",
            "previewPath": "{{locale}}/blog/posts",
            "trailingSlash": false,
            "defaultLocale": "en",
            "locales": [
                {
                    "title": "English",
                    "locale": "en",
                    "path": "en"
                },
                {
                    "title": "Czech",
                    "locale": "cs",
                    "path": "cs"
                }
            ]
        },
        {
            "title": "Projects",
            "path": "[[workspace]]/www/content/projects",
            "defaultLocale": "en",
            "locales": [
                {
                    "title": "English",
                    "locale": "en",
                    "path": "en"
                },
                {
                    "title": "Czech",
                    "locale": "cs",
                    "path": "cs"
                }
            ]
        },
        {
            "title": "Resume",
            "path": "[[workspace]]/www/content/resume",
            "disableCreation": true
        },
        {
            "title": "Privacy policy",
            "path": "[[workspace]]/www/content/privacy-policy",
            "disableCreation": true
        }
    ],
    "frontMatter.taxonomy.contentTypes": [
        {
            "name": "blog",
            "pageBundle": false,
            "slugTemplate": "{{year}}-{{month}}-{{day}}-{{seoTitle}}",
            "fields": [
                {
                    "name": "title",
                    "title": "Title",
                    "type": "string",
                    "required": true
                },
                {
                    "name": "description",
                    "title": "Description",
                    "type": "string",
                    "required": true
                },
                {
                    "name": "tags",
                    "title": "Tags",
                    "type": "dataFile",
                    "dataFileId": "tags",
                    "dataFileKey": "id",
                    "multiple": true,
                    "dataFileValue": "id",
                    "required": true
                },
                {
                    "name": "locale",
                    "title": "Locale",
                    "type": "choice",
                    "choices": ["cs", "en"],
                    "required": true
                },
                {
                    "name": "draft",
                    "title": "Draft?",
                    "type": "draft",
                    "default": true
                },
                {
                    "name": "featured",
                    "title": "Feautred?",
                    "type": "boolean",
                    "default": false
                },
                {
                    "name": "image",
                    "title": "Image",
                    "type": "image",
                    "required": false,
                    "isPreviewImage": true
                },
                {
                    "name": "pubDate",
                    "title": "Published date",
                    "type": "datetime",
                    "required": true,
                    "default": "{{now}}",
                    "isPublishDate": true
                },
                {
                    "name": "modDate",
                    "title": "Modified date",
                    "type": "datetime",
                    "isModifiedDate": true
                }
            ]
        },
        {
            "name": "resume",
            "pageBundle": false,
            "fields": [
                {
                    "title": "Title",
                    "name": "title",
                    "type": "string"
                },
                {
                    "title": "Description",
                    "name": "description",
                    "type": "string"
                },
                {
                    "title": "Current position",
                    "name": "currentPosition",
                    "type": "string"
                }
            ]
        },
        {
            "name": "privacy-policy",
            "pageBundle": false,
            "fields": [
                {
                    "title": "Title",
                    "name": "title",
                    "type": "string"
                },
                {
                    "title": "Effective date",
                    "name": "effectiveDate",
                    "type": "datetime"
                }
            ]
        },
        {
            "name": "project",
            "pageBundle": false,
            "slugTemplate": "{{seoTitle}}",
            "fields": [
                {
                    "title": "Title",
                    "name": "title",
                    "type": "string"
                },
                {
                    "title": "Source URL",
                    "name": "sourceUrl",
                    "type": "string"
                },
                {
                    "name": "tags",
                    "title": "Tags",
                    "type": "dataFile",
                    "dataFileId": "projectTags",
                    "dataFileKey": "id",
                    "multiple": true,
                    "dataFileValue": "id",
                    "required": true
                },
                {
                    "title": "Locale",
                    "name": "locale",
                    "type": "string"
                },
                {
                    "title": "Project start",
                    "name": "startDate",
                    "type": "datetime"
                },
                {
                    "title": "Active?",
                    "name": "active",
                    "type": "boolean"
                }
            ]
        }
    ],
    "frontMatter.git.enabled": true,
    "frontMatter.git.commitMessage": "Synced content",
    "frontMatter.content.snippets": {
        "Note callout": {
            "description": "",
            "body": ["<Callout type=\"note\" title=\"\"", "", "</Callout>"],
            "fields": []
        },
        "Success callout": {
            "description": "",
            "body": ["<Callout type=\"success\" title=\"\"", "", "</Callout>"],
            "fields": []
        },
        "Caution callout": {
            "description": "",
            "body": ["<Callout type=\"caution\" title=\"\"", "", "</Callout>"],
            "fields": []
        },
        "Danger callout": {
            "description": "",
            "body": ["<Callout type=\"danger\" title=\"\"", "", "</Callout>"],
            "fields": []
        },
        "Tip callout": {
            "description": "",
            "body": ["<Callout type=\"tip\" title=\"\"", "", "</Callout>"],
            "fields": []
        }
    }
}
