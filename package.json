{
    "name": "daliborhon.dev",
    "type": "module",
    "version": "1.0.0",
    "scripts": {
        "init": "pnpm run wrangler:types && pnpm run build",
        "dev": "astro dev",
        "dev:host": "astro dev --host",
        "build": "pnpm run compile:paraglide && astro build",
        "build-and-preview": "astro build && pnpm run preview",
        "preview": "wrangler pages dev ./dist --compatibility-flags='nodejs_compat' --live-reload --port 4321",
        "astro": "astro",
        "sync": "astro sync",
        "prettier:check": "prettier . --check",
        "prettier:format": "prettier . --write",
        "build:index": "pagefind --site dist",
        "generate:graphql": "graphql-codegen -r dotenv/config --config codegen.ts",
        "generate:graphql-watch": "graphql-codegen --watch -r dotenv/config --config codegen.ts",
        "generate:graphql-check": "graphql-codegen --check -r dotenv/config --config codegen.ts",
        "compile:paraglide": "paraglide-js compile --project ./project.inlang",
        "wrangler:types": "wrangler types ./src/worker-configuration.d.ts",
        "deps:check-update": "ncu -x @astrojs/cloudflare",
        "deps:update": "ncu -u -x @astrojs/cloudflare && pnpm install"
    },
    "dependencies": {
        "@apollo/client": "^3.9.10",
        "@astrojs/cloudflare": "9.2.0",
        "@astrojs/react": "^3.1.0",
        "@astrojs/rss": "^4.0.5",
        "@astrojs/tailwind": "^5.1.0",
        "@caisy/rich-text-astro-renderer": "^1.0.2",
        "@caisy/rich-text-html-parser": "^0.1.2",
        "@formkit/auto-animate": "^0.8.1",
        "@hcaptcha/react-hcaptcha": "^1.10.1",
        "@iconify-json/bi": "^1.1.23",
        "@iconify-json/devicon": "^1.1.36",
        "@iconify-json/tabler": "^1.1.109",
        "@inlang/paraglide-js-adapter-astro": "^0.1.19",
        "@pagefind/default-ui": "^1.1.0",
        "@resvg/resvg-js": "^2.6.2",
        "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
        "@tailwindcss/typography": "^0.5.12",
        "@unpic/astro": "^0.0.44",
        "apollo-link-scalars": "^4.0.3",
        "astro": "^4.5.13",
        "astro-icon": "^1.1.0",
        "astro-pagefind": "^1.5.0",
        "focus-trap": "^7.5.4",
        "graphql": "^16.8.1",
        "graphql-scalars": "^1.23.0",
        "graphql-tag": "^2.12.6",
        "iso-639-1": "^3.1.2",
        "js-cookie": "^3.0.5",
        "jsdom": "^24.0.0",
        "pagefind": "^1.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.51.2",
        "shikiji": "^0.10.2",
        "tailwindcss": "^3.4.3",
        "tippy.js": "^6.3.7"
    },
    "devDependencies": {
        "@astrojs/sitemap": "^3.1.2",
        "@cloudflare/workers-types": "^4.20240329.0",
        "@graphql-codegen/cli": "^5.0.2",
        "@graphql-codegen/client-preset": "^4.2.5",
        "@graphql-codegen/introspection": "^4.0.3",
        "@graphql-codegen/typescript-generic-sdk": "^4.0.1",
        "@graphql-codegen/typescript-graphql-request": "^6.2.0",
        "@inlang/paraglide-js": "1.3.5",
        "@parcel/watcher": "^2.4.1",
        "@types/js-cookie": "^3.0.6",
        "@types/js-yaml": "^4.0.9",
        "@types/jsdom": "^21.1.6",
        "@types/node": "^20.12.2",
        "@types/react": "^18.2.74",
        "@types/react-dom": "^18.2.23",
        "@types/slug": "^5.0.8",
        "autoprefixer": "^10.4.19",
        "js-yaml": "^4.1.0",
        "md-to-pdf": "^5.2.4",
        "postcss-import": "^16.1.0",
        "postcss-preset-env": "^9.5.4",
        "prettier": "^3.2.5",
        "prettier-plugin-astro": "^0.13.0",
        "prettier-plugin-tailwindcss": "^0.5.13",
        "satori": "^0.10.13",
        "sharp": "^0.33.3",
        "slug": "^9.0.0",
        "tsx": "^4.7.1",
        "typescript": "^5.4.3",
        "vite": "^5.2.7",
        "wrangler": "^3.43.0"
    },
    "packageManager": "pnpm@8.15.5+sha256.4b4efa12490e5055d59b9b9fc9438b7d581a6b7af3b5675eb5c5f447cee1a589"
