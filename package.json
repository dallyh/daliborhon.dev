{
    "name": "daliborhon.dev",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "prettier:check": "prettier . --check",
        "prettier:format": "prettier . --write",
        "bootstrap": "pnpm install && pnpm --filter daliborhon.dev-studio generate:types",
        "dev": "turbo dev",
        "dev:studio": "pnpm --filter daliborhon.dev-studio dev",
