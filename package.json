{
    "name": "daliborhon.dev",
    "type": "module",
    "version": "1.0.0",
    "scripts": {
        "dev": "astro dev",
        "dev-host": "astro dev --host",
        "build": "paraglide-js compile --project ./project.inlang && astro build",
        "build-and-preview": "astro build && wrangler pages dev ./dist --compatibility-flags='nodejs_compat' --live-reload --port 4321",
        "preview": "wrangler pages dev ./dist --compatibility-flags='nodejs_compat' --live-reload --port 4321",
        "astro": "astro",
        "sync": "astro sync",
        "prettier:check": "prettier . --check",
        "prettier:format": "prettier . --write",
        "generate:locale-types": "tsx ./scripts/generate-i18n-locale-types.ts",
        "build:index": "pagefind --site dist",
        "generate:graphql": "graphql-codegen -r dotenv/config --config codegen.ts",
        "generate:watch": "graphql-codegen --watch -r dotenv/config --config codegen.ts",
        "generate:check": "graphql-codegen --check -r dotenv/config --config codegen.ts",
        "compile:paraglide": "paraglide-js compile --project ./project.inlang"
    },
    "dependencies": {
        "@apollo/client": "^3.9.8",
        "@astrojs/check": "^0.5.9",
        "@astrojs/cloudflare": "^9.1.0",
        "@astrojs/react": "^3.1.0",
        "@astrojs/tailwind": "^5.1.0",
        "@caisy/rich-text-astro-renderer": "^1.0.2",
        "@floating-ui/dom": "^1.6.3",
        "@formkit/auto-animate": "^0.8.1",
        "@hcaptcha/react-hcaptcha": "^1.10.1",
        "@iconify-json/bi": "^1.1.23",
        "@iconify-json/devicon": "^1.1.36",
        "@iconify-json/tabler": "^1.1.109",
        "@inlang/paraglide-js-adapter-astro": "^0.1.14",
        "@pagefind/default-ui": "^1.0.4",
        "@resvg/resvg-js": "^2.6.0",
        "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
        "@tailwindcss/typography": "^0.5.10",
        "@unpic/astro": "^0.0.44",
        "apollo-link-scalars": "^4.0.3",
        "astro": "^4.5.8",
        "astro-icon": "^1.1.0",
        "astro-pagefind": "^1.5.0",
        "focus-trap": "^7.5.4",
        "graphql": "^16.8.1",
        "graphql-scalars": "^1.23.0",
        "i18next": "^23.10.1",
        "pagefind": "^1.0.4",
        "postcss-import": "^16.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.51.1",
        "react-i18next": "^14.1.0",
        "react-refresh": "^0.14.0",
        "sharp": "^0.33.2",
        "shikiji": "^0.10.2",
        "tailwindcss": "^3.4.1",
        "tippy.js": "^6.3.7",
        "typescript": "^5.4.3"
    },
    "devDependencies": {
        "@astrojs/sitemap": "^3.1.1",
        "@cloudflare/workers-types": "^4.20240320.1",
        "@graphql-codegen/cli": "^5.0.2",
        "@graphql-codegen/client-preset": "^4.2.4",
        "@graphql-codegen/introspection": "^4.0.3",
        "@graphql-codegen/typescript-generic-sdk": "^4.0.1",
        "@graphql-codegen/typescript-graphql-request": "^6.2.0",
        "@inlang/paraglide-js": "1.3.3",
        "@parcel/watcher": "^2.4.1",
        "@types/js-yaml": "^4.0.9",
        "@types/react": "^18.2.67",
        "@types/react-dom": "^18.2.22",
        "@types/slug": "^5.0.8",
        "autoprefixer": "^10.4.19",
        "i18next-fs-backend": "^2.3.1",
        "iso-639-1": "^3.1.2",
        "js-yaml": "^4.1.0",
        "postcss-preset-env": "^9.5.2",
        "prettier": "^3.2.5",
        "prettier-plugin-astro": "^0.13.0",
        "prettier-plugin-tailwindcss": "^0.5.12",
        "quicktype": "^23.0.107",
        "sass": "^1.72.0",
        "satori": "^0.10.13",
        "slug": "^9.0.0",
        "tsx": "^4.7.1",
        "wrangler": "^3.36.0"
    }
