    "devDependencies": {
        "@types/js-yaml": "^4.0.9",
        "@types/node": "^20.12.2",
        "eslint": "^8.6.0",
        "js-yaml": "^4.1.0",
        "prettier": "^3.2.5",
        "prettier-plugin-astro": "^0.13.0",
        "prettier-plugin-tailwindcss": "^0.5.13",
        "rimraf": "^5.0.5",
        "tsx": "^4.7.1",
        "typescript": "^5.4.3",
        "turbo": "^1.13.2"
    },
    "packageManager": "pnpm@8.15.6"
}
