---
import RichTextRenderer from "@caisy/rich-text-astro-renderer";
import CaisyCodeBlock from "./CaisyCodeBlock.astro";

export interface Props {
    richTextJson?: any;
}

const { richTextJson } = Astro.props;

const overwrites = {
    codeBlock: CaisyCodeBlock,
};
---

<RichTextRenderer node={richTextJson} overwrites={overwrites} />
